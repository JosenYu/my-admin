port default {\n  components: {\n    NavItem: () => import(\"./NavItem\")\n  },\n  props: {\n    // 整体的路由\n    router: {\n      type: Object,\n      require: true\n    },\n    // 路径\n    path: {\n      type: String\n    }\n  },\n  mounted() {\n    console.log(this.router);\n  }\n};\n</script>\n\n<style lang=\"stylus\" scoped></style>\n"]}]}